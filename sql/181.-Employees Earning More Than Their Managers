/* 

Difficulty : Easy 

*/


# Method 1 (Sub Query )

SELECT name AS Employee
FROM Employee AS E
WHERE salary > (SELECT salary FROM Employee WHERE E.managerId IS NOT NULL AND id = E.managerId )


# Method 2 (Self Join)

SELECT E1.name AS Employee
FROM Employee AS E1
INNER JOIN Employee AS E2
ON E1.managerId = E2.id AND E1.salary > E2.salary